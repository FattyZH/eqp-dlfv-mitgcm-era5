gluemncbig -o ./run/surfDiag.nc ./run/mnc_exp_*/surfDiag*.t*.nc
gluemncbig -o ./run/phiHyd.nc ./run/mnc_exp_*/phiHyd*.t*.nc
gluemncbig -o ./run/phiHydLow.nc ./run/mnc_exp_*/phiHydLow*.t*.nc